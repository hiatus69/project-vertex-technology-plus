{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "serviceName": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "integer"
    },
    "features": {
      "type": "blocks"
    },
    "serviceImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "displayOrder": {
      "type": "integer"
    },
    "isPromotion": {
      "type": "boolean",
      "default": false
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "service"
    }
  }
}
