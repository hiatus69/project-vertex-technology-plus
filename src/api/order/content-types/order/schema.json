{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Status_order": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "Waiting in queue",
        "Accepting order",
        "Installing",
        "Completed",
        "Cancel"
      ]
    },
    "customerNotes": {
      "type": "text",
      "unique": false
    },
    "contactName": {
      "type": "string"
    },
    "installationAddress": {
      "type": "text"
    },
    "contactPhone": {
      "type": "string"
    },
    "bookingDate": {
      "type": "datetime"
    },
    "completionProof": {
      "type": "component",
      "component": "shared.completion-proof",
      "repeatable": false
    },
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "order"
    },
    "employee": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "assigned_orders"
    }
  }
}
